<Window x:Class="ProcWatcher.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ProcWatcher"
        mc:Ignorable="d"
        Title="ProcWatcher" Height="300" Width="600"
        WindowStartupLocation="CenterScreen">
    <Grid>
        <TextBlock Margin="20, 5 ,0 ,0"><Bold>[ProcessId] Process Name</Bold></TextBlock>
        <ListBox Margin="20, 25, 20, 10" Name="ProcessBox" HorizontalContentAlignment="Stretch">
            
        </ListBox>
    </Grid>
</Window>
